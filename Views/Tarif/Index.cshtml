integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous"> *@
@{
    Layout = "~/Views/Shared/_LayoutAlternative.cshtml";
}
@model IEnumerable<Ahtapot_Recipe.Models.TarifModel>
<br><br><br><br>
<a style="float: right" href="/Tarif/Ekle">
    <input type="button" class="btn btn-success" value="Go Somewhere Else" />
</a>
<br><br><br><br>
<div class="list-group">
    @foreach (var item in Model)
    {
        <a href="#" class="list-group-item list-group-item-action" aria-current="true">
            <div class="d-flex w-100 justify-content-between">
                <h5 class="mb-1">@item.YemekAdi.ToString()</h5>
                <small>@item.OlusturulmaTarihi.ToString()</small>
            </div>
            <p class="mb-1">@item.Yapilisi.ToString()</p>
            <small>@item.Malzemeler.ToString()</small>
        </a>
    }
</div>